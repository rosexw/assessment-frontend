<div class="container">
  <main>
    <h2>Product Page - Layout</h2>
    {{#each products as |product|}}
      <h4>{{product.name}} {{link-to "EXPAND" 'products.show' product.id}}</h4>

      <table>
        {{#each product.productDataSubset as |data|}}
          <tr>
            <td>{{data.ticker}}</td>
            <td>{{data.comp_name}}</td>
            <td>{{data.per_end_date}}</td>
          </tr>
        {{/each}}
      </table>
    {{/each}}
  </main>

  <aside>
    ASIDE
  </aside>
</div>

{{outlet}}
